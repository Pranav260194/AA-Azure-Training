{
	"name": "RemoveDuplicates",
	"properties": {
		"folder": {
			"name": "JG186076"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "JG186076_RMDupCust",
						"type": "DatasetReference"
					},
					"name": "RmDupCust"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "NoDup_Cust",
						"type": "DatasetReference"
					},
					"name": "CustNoDup"
				}
			],
			"transformations": [
				{
					"name": "aggregate1"
				},
				{
					"name": "SortonCustID"
				}
			],
			"scriptLines": [
				"source(output(",
				"          CustomerID as string,",
				"          FirstName as string,",
				"          LastName as string,",
				"          Email as string,",
				"          Address as string,",
				"          City as string,",
				"          Country as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> RmDupCust",
				"RmDupCust aggregate(groupBy(CustomerID),",
				"     each(match(name!='CustomerID'), $$ = last($$))) ~> aggregate1",
				"aggregate1 sort(asc(CustomerID, true)) ~> SortonCustID",
				"SortonCustID sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          CustomerID as string,",
				"          FirstName as string,",
				"          LastName as string,",
				"          Email as string,",
				"          Address as string,",
				"          City as string,",
				"          Country as string",
				"     ),",
				"     partitionFileNames:['CustNoDup.Csv'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> CustNoDup"
			]
		}
	}
}